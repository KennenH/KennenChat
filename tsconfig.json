{
  "compilerOptions": {
    "target": "es5", 
    "module": "esnext", 
    "jsx": "react-jsx", 
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true, 
    "noEmit": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
  },
  "include": ["src"]
}
